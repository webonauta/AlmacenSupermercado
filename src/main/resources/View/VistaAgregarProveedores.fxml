<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="662.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.VistaAgregarProveedoresController">
